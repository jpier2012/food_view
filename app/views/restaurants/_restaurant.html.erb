<%= form_for @restaurant do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :address %>
    <%= f.text_field :address %><br>

    <%= f.label :cuisine %>
    <%= f.text_field :cuisine %><br>

    <%= f.label :dress_code %>
    <%= f.text_field :dress_code %><br>

    <%= f.label :outdoor_seating? %>
    <%= f.check_box :outdoor_seating? %><br>

    <%= f.label :child_friendly? %>
    <%= f.check_box :child_friendly? %><br>

    <%= f.label :open_bar? %>
    <%= f.check_box :open_bar? %><br>
    
    <%= f.label :byob?, "BYOB?" %>
    <%= f.check_box :byob? %><br>
    
    <%= f.submit %><br>
<% end %>
