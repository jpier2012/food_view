<!DOCTYPE html>
<html>
  <head>
    <title>FoodView</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= flash[:notice] if flash[:notice] %></p>
    <p class="alert"><%= flash[:alert] if flash[:alert] %></p>
    <%= yield %>
    <%= render "nav" %>
  </body>
</html>
